import std.io.stdout;

func main() -> int {
    const x: int = 5;
    let y: int = 10;

    let z: char = 'a';

    loop(const i : std.range(1, 100)) {
        if(i % 15 == 0) println("FizzBuzz");
        else if(i % 3 == 0) {
            println("Fizz");
        } else if(i % 5 == 0) {
            println("Buzz");
        } else {
            i |> std.toString |> println;
        }
    }

    // Statements are expressions
    println(if true { "wow" } else { "wow 2" });
}